			</form>
					<div>
						<div class="acad">
						Projects:
					<button onclick=addProject()>Add Project</button></div>
					<div class="projects">
						<%for ( var i = 0; i < projects.length; i++ ){
							var identity = "pro"+(i+1);%>
							<form method="post" action="/deleteProject">
								<div class="project" id=<%=identity%>>
									<input type="text" class="invisible" value="<%=projects[i].id%>" name="id">
									<button type="submit">Delete</button>
									<p><b><%=projects[i].Title%></b><i> (<%=projects[i].StartDate%> to <%=projects[i].EndDate%>) </i></p>
									<p><%=projects[i]['Company Name']%></p>
									<p><%=projects[i].Description%></p>
								</div>
							</form>
