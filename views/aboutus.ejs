	<div class="content">
		
		


		<br/><br/>
		
		<form action = "/companyHome/aboutus" method = "post">
			<label>Company Name: </label> <input type = "text" name = "companyname" value = "<%=data['Company name'] %>"> <br/><br/>
			<label>About: </label> <textarea name = "about" >
				<%=data.About %>
			</textarea> <br/>
			<label>Established Date: </label> <input type = "date" name = "estdate" value = "<%=data['Established Date'] %>"> <br/>
			<label>No of employees: </label> <input type = "number" name = "empcount" value = "<%=data['No of Employee'] %>"> <br/>
			<input type = "submit">
		</form>
	</div>

</body>
